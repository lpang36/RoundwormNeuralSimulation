import java.util.Comparator;  

public class InputKey implements Comparable <InputKey> {
    int index;
    double time;
    double value;
    InputKey(double b, double c) {
      //index = a;
      time = b;
      value = c;
      if (b==Double.POSITIVE_INFINITY||b==Double.NEGATIVE_INFINITY) {
        value = 0;
      }
    }
    @Override
    public int compareTo(InputKey A) {
      if (A.time<this.time) 
        return 1;
      if (this.time<A.time)
        return -1;
      return 0;
    }
  }